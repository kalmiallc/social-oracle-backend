{
  "name": "social-oracle-api",
  "version": "1.0.0",
  "description": "Social oracle API.",
  "author": "Kalmia",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tsc && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "tsc": "tsc",
    "dev": "cross-env IS_OFFLINE=true npm run start:dev",
    "test": "cross-env APP_ENV=test LOG_TARGET=none jest --detectOpenHandles --runInBand --forceExit --silent",
    "test:logging": "cross-env APP_ENV=test LOG_TARGET=console jest --detectOpenHandles --runInBand --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --forceExit",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy:prod": "serverless deploy --stage production",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:dev": "serverless deploy --stage dev",
    "db-upgrade": "node -r ts-node/register ./src/scripts/db/upgrade-db",
    "db-downgrade": "node -r ts-node/register ./src/scripts/db/downgrade-db",
    "db-seed": "node -r ts-node/register ./src/scripts/db/seed-db",
    "db-unseed": "node -r ts-node/register ./src/scripts/db/unseed-db",
    "db-rebuild": "node -r ts-node/register ./src/scripts/db/rebuild-db",
    "db-rebuild-test": "node -r ts-node/register ./src/scripts/db/rebuild-test-db"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.693.0",
    "@aws-sdk/client-s3": "^3.686.0",
    "@aws-sdk/client-secrets-manager": "^3.682.0",
    "@aws-sdk/client-sqs": "^3.693.0",
    "@aws-sdk/s3-request-presigner": "^3.686.0",
    "@nestjs/common": "^10.4.8",
    "@nestjs/core": "^10.4.8",
    "@nestjs/platform-express": "^10.4.8",
    "@rawmodel/core": "^3.3.4",
    "@rawmodel/parsers": "^3.3.4",
    "@rawmodel/validators": "^3.3.4",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.7.7",
    "colors": "^1.4.0",
    "cron-parser": "^4.9.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.4",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mysql2": "^3.11.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-mysql-migrate": "^1.1.2",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.4.8",
    "@types/aws-serverless-express": "^3.3.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.13",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-no-async-foreach": "^0.1.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.1",
    "jest": "29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "serverless": "^3.39.0",
    "serverless-plugin-resource-tagging": "^1.2.0",
    "serverless-webpack": "^5.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "testTimeout": 1800000,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "rootDir": "src",
    "testPathIgnorePatterns": [
      "dist/*"
    ],
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "preset": "ts-jest",
    "verbose": true
  }
}
